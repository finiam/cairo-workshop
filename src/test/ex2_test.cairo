use cairo_workshop::ex2;

#[ignore]
#[test]
fn test_fib() {
 let input = 0;
 let expected_value = 1;

  assert(ex2::fib(input) == expected_value, 'not the expected value');
}

#[ignore]
#[test]
fn test_fib() {
 let input = 1;
 let expected_value = 1;

  assert(ex2::fib(1) == expected_value, 'not the expected value');
}

#[ignore]
#[test]
fn test_fib() {
 let input = 3;
 let expected_value = 2;

  assert(ex2::fib(1) == expected_value, 'not the expected value');
}
